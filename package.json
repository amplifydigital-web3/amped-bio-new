{
  "name": "amped-bio",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*"
  ],
  "engines": {
    "node": ">=22.9.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "packageManager": "yarn@1.22.22"
}