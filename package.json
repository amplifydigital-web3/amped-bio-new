{
  "name": "ampedbio",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=22.9.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "e2e:test": "pnpm --filter client e2e:test"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.15.4"
}